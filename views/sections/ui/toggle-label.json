{
  "wrapper": "div.flex.flex-wrap.my-8",
  "sections": {
    "checkbox": {
      "type": "ui/toggle/checkbox",
      "settings": {
        "toggle_selector": "{{section.settings.toggle_selector}}"
      }
    },
    "header": {
      "type": "ui/toggle/label",
      "settings": {
        "toggle_selector": "{{section.settings.toggle_selector}}",
        "name": "{{section.settings.name}}",
        "hint": "{{section.settings.hint}}",
        "icon": "{{section.settings.icon}}"
      }
    },
    "main": {
      "type": "ui/toggle/on/content",
      "settings": {
        "content": "{{section.settings.content}}"
      }
    }
  },
  "order": [
    "checkbox",
    "header",
    "main"
  ]
}
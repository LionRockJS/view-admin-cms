{
  "wrapper": "div.container.container-columns",
  "sections": {
    "script": {
      "type": "#external_scripts",
      "settings": {
        "value": [
          "https://unpkg.com/turndown/dist/turndown.js",
          "https://cdn.jsdelivr.net/npm/marked/marked.min.js"
        ]
      }
    },
    "defer_script": {
      "type": "#defer_scripts",
      "settings": {
        "value": [
          "admin/pages/edit.mjs"
        ]
      }
    },
    "main": {
      "type": "admin/pages/default/edit",
      "settings": {
        "controller": "{{page_type}}",
        "block_types": ""
      }
    },
    "content": {
      "type": "ui/content",
      "settings": {
        "breadcrumb": [
          {
            "label": "Home",
            "url": "/admin"
          },
          {
            "label": "{{page_type | capitalize}}",
            "url": "/admin/pages/list/{{page_type}}"
          },
          {
            "label": "Edit {{ item.name }}",
            "url": "/admin/{{ controller_slug | default: 'pages' }}/{{ item.id }}"
          }
        ],
        "section_1": "page/content",
        "section_2": "admin/navigation"
      }
    }
  },
  "order": [
    "script",
    "defer_script",
    "main",
    "content"
  ]
}
{
  "sections": {
    "script": {
      "type": "#external_scripts",
      "settings": {
        "value": [
          "https://unpkg.com/turndown/dist/turndown.js",
          "https://cdn.jsdelivr.net/npm/marked/marked.min.js"
        ]
      }
    },
    "defer_script": {
      "type": "#defer_scripts",
      "settings": {
        "value": [
          "admin/pages/edit.mjs"
        ]
      }
    },
    "main": {
      "type": "ui/workspace",
      "settings": {
        "section_main": "admin/pages/default/edit",
        "title": "{{ page_type | capitalize }}",
        "controller_slug": "pages",
        "breadcrumb": "Home:/admin,{{page_type | capitalize}}:/admin/pages/list/{{page_type}}, Edit {{ item.name }}:/admin/pages/{{ item.id }}",
        "section_search": "",
        "section_header": "",
        "section_body": "page/content",
        "section_menu": "admin/navigation",
        "section_navigation": "admin/navigation-icon"
      }
    }
  },
  "order": [
    "script",
    "defer_script",
    "main"
  ]
}